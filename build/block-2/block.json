{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/block-2",
  "version": "1.0.0",
  "title": "Image à gauche et texte à droite",
  "category": "widgets",
  "icon": "screenoptions",
  "description": "Bloc avec une image à gauche et texte à droite.",
  "supports": {
    "html": false
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": ".content"
    },
    "pictureID": {
      "type": "number",
      "default": null
    },
    "pictureURL": {
      "type": "string",
      "source": "attribute",
      "attribute": "src",
      "selector": "img"
    },
    "pictureAlt": {
      "type": "string",
      "source": "attribute",
      "attribute": "alt",
      "selector": "img"
    }
  },
  "keywords": [
    "Bloc deux",
    "Nathan Dubocage"
  ],
  "textdomain": "nathan.dubocage.gutenberg",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}